<?xml version="1.0" encoding="UTF-8"?> <xsd:schema xmlns="http://schema.gspt.net/interactive-voice-response/services" targetNamespace="http://schema.gspt.net/interactive-voice-response/services"    xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"     attributeFormDefault="unqualified">     <xsd:complexType name="OrderStatusRequestType">         <xsd:annotation>             <xsd:documentation xml:lang="en">Order Status Request</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element name="Version" type="xsd:string">                 <xsd:annotation>                     <xsd:documentation>Version Number. The default value is "1.0".</xsd:documentation>                 </xsd:annotation>             </xsd:element>             <xsd:element name="Client" type="xsd:string">                 <xsd:annotation>                     <xsd:documentation>Client Name, for example: "Tuvox","Webstore"</xsd:documentation>                 </xsd:annotation>             </xsd:element>             <xsd:choice>                 <xsd:element name="WebOrderId" type="xsd:string">                     <xsd:annotation>                         <xsd:documentation>Web Order Id</xsd:documentation>                     </xsd:annotation>                 </xsd:element>                 <xsd:element name="BillingPhoneNumber" type="xsd:string">                     <xsd:annotation>                         <xsd:documentation>Billing Phone Number</xsd:documentation>                     </xsd:annotation>                 </xsd:element>             </xsd:choice>         </xsd:sequence>         <xsd:attribute name="ChannelId" type="xsd:string">             <xsd:annotation>                 <xsd:documentation>The name assigned to the channel. Examples: 'FOG', 'TRUJP'. This                     will be required as a top level attribute for all Order Service requests.                 </xsd:documentation>             </xsd:annotation>         </xsd:attribute>     </xsd:complexType>     <xsd:element name="OrderStatusRequest" type="OrderStatusRequestType" nillable="false"/> </xsd:schema> 
